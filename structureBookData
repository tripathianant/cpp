#include<stdio.h>
int main()
{
     struct book
     {
         char title[20];
         char author[20];
         char pubc[20];
         float price;
     };
     struct book b1,b2;
     printf("ENTER TITLE , AUTHOR , PUBLICATION AND PRICE OF TWO BOOKS \n");
     scanf("%s%f%d",b1.name,&b1.price,&b1.pages);
     scanf("%s%f%d",b2.name,&b2.price,&b2.pages);
     printf("YOUE RESULT IS \n");
     printf("%s  %f  %d",b1.name,b1.price,b1.pages);
     printf("\n%s  %f  %d",b2.name,b2.price,b2.pages);
}